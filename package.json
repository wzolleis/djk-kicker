{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:db": "prisma generate",
    "generate:css": "tailwindcss -i ./styles/app.css -o ./app/styles/tailwind.css",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma db seed",
    "start": "cross-env NODE_ENV=production netlify dev",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@netlify/functions": "^1.0.0",
    "@prisma/client": "^4.7.1",
    "@remix-run/netlify": "^1.7.3",
    "@remix-run/node": "^1.7.3",
    "@remix-run/react": "^1.7.3",
    "@tailwindcss/forms": "^0.5.3",
    "@types/nodemailer": "^6.4.6",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "framer-motion": "^7.7.2",
    "jose": "^4.11.0",
    "luxon": "^3.1.0",
    "moment": "^2.29.4",
    "netlify": "^13.1.0",
    "nodemailer": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "tailwindcss": "^3.1.8",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@remix-run/dev": "^1.7.3",
    "@remix-run/eslint-config": "^1.7.3",
    "@remix-run/serve": "^1.7.3",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.6",
    "@types/luxon": "^3.0.1",
    "@types/node": "^14.18.33",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitest/coverage-c8": "^0.23.1",
    "autoprefixer": "^10.4.08",
    "binode": "^1.0.5",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^7.6.6",
    "msw": "^0.47.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.4"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "engines": {
    "node": ">=14"
  }
}
