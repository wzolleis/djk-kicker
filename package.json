{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:db && npm run build:css && npm run build:remix",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:db": "prisma generate",
    "generate:css": "tailwindcss -i ./styles/app.css -o ./app/styles/tailwind.css",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma db seed",
    "start": "cross-env NODE_ENV=production geparkt/netlify dev",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "ncu": {
    "dependency_update": "npx npm-check-updates -u",
    "ncu_global": "npm install -g npm-check-updates",
    "ncu_home": "https://github.com/raineorshine/npm-check-updates"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@heroicons/react": "^2.0.18",
    "@netlify/functions": "^1.6.0",
    "@prisma/client": "^5.0.0",
    "@remix-run/netlify": "^1.19.1",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/v1-meta": "^0.1.1",
    "@remix-run/v1-route-convention": "^0.1.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tanstack/react-query": "^4.32.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "driftmail": "^1.1.10",
    "framer-motion": "^10.13.0",
    "jose": "^4.14.4",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "moment": "^2.29.4",
    "netlify": "^13.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.2",
    "tailwindcss": "^3.3.3",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@remix-run/serve": "^1.19.1",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.44.0",
    "@types/lodash": "^4.14.195",
    "@types/luxon": "^3.3.1",
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "happy-dom": "^10.5.2",
    "msw": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "engines": {
    "node": ">=18"
  }
}
